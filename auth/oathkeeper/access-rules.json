[
  {
    "id": "api:graph-playground",
    "upstream": {
      "preserve_host": true,
      "url": "http://192.168.49.1:8000"
    },
    "match": {
      "url": "http://<*>.localhost:8080/<**>",
      "methods": [
        "GET",
        "POST"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  },
  {
    "id": "api:graph-query",
    "upstream": {
      "preserve_host": true,
      "url": "http://192.168.49.1:8000"
    },
    "match": {
      "url": "http://<[^/]+>/api/query",
      "methods": [
        "GET",
        "POST"
      ]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  }
]