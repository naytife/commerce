{
    "name": "template-1",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev --port 3080",
        "build": "NODE_OPTIONS='--max-old-space-size=2048' vite build",
        "build:optimized": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=2048' vite build --minify terser",
        "build:analyze": "npm run build:optimized && npx vite-bundle-analyzer dist",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test"
    },
    "devDependencies": {
        "@internationalized/date": "^3.5.6",
        "@playwright/test": "^1.45.3",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.8.2",
        "@sveltejs/vite-plugin-svelte": "^4.0.1",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@tailwindcss/typography": "^0.5.15",
        "@types/eslint": "^9.6.0",
        "@types/node": "^24.0.1",
        "autoprefixer": "^10.4.16",
        "bits-ui": "^2.8.1",
        "clsx": "^2.1.1",
        "cmdk-sv": "^0.0.19",
        "embla-carousel-svelte": "^8.3.1",
        "eslint": "^9.7.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.36.0",
        "flowbite": "^2.5.2",
        "flowbite-svelte": "^0.48.0",
        "formsnap": "^1.0.1",
        "globals": "^15.0.0",
        "lucide-svelte": "^0.454.0",
        "mode-watcher": "^0.4.1",
        "paneforge": "^0.0.6",
        "prettier": "^3.3.2",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.2.7",
        "svelte-check": "^4.0.0",
        "svelte-sonner": "^1.0.5",
        "sveltekit-superforms": "^2.20.0",
        "tailwind-merge": "^2.6.0",
        "tailwind-variants": "^0.2.1",
        "tailwindcss": "^3.4.13",
        "terser": "^5.42.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.0.0",
        "vaul-svelte": "^0.3.2",
        "vite": "^5.4.11",
        "vitest": "^2.0.4",
        "zod": "^3.23.8"
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@popperjs/core": "^2.11.8",
        "@stripe/stripe-js": "^7.3.1",
        "@urql/core": "^5.1.1",
        "@urql/svelte": "^4.2.3",
        "graphql": "^16.11.0",
        "stripe": "^18.2.1"
    }
}
